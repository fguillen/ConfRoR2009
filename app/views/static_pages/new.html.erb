<% title 'New Page' %>

<% form_for(@static_page) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<p class="notes">Fields marked with <span class="required">*</span> are required fields</p>

<p>
  <%= link_to "&laquo; Go back to pages", static_pages_path %>
</p>
